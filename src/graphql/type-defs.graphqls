type Query {
  hello: String!

  createUser(
    username: String!
    email: String!
    first_name: String!
    last_name: String!
    password: String!
  ): String!
}
type Mutation {
  createUser: CreateUser
}

type CreateUser {
  createUser(
    username: String!
    email: String!
    first_name: String!
    last_name: String!
    password: String!
  ): String!
}
